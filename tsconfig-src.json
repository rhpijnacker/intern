{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "declaration": true,
    "downlevelIteration": true,
    "lib": ["dom", "es2015"],
    "module": "commonjs",
    "preserveConstEnums": true,
    "removeComments": true,
    "sourceMap": true,
    "target": "es5"
  },
  "include": [
    "src/**/*.ts"
  ],
  "exclude": [
    "src/core/browser/**",
    "src/core/lib/browser/**"
  ]
}
